            <!-- Toggle button -->
            <button class="navbar-toggler" type="button" data-mdb-toggle="collapse" data-mdb-target="#navbarButtonsExample" aria-controls="navbarButtonsExample" aria-expanded="false" aria-label="Toggle navigation">
        <i class="fas fa-bars"></i>
      </button>

            <!-- Collapsible wrapper -->
            <div class="collapse navbar-collapse" id="navbarButtonsExample">
                <!-- Left links -->
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link text-light" href="#">Presupuestos para proyectos de TI.</a>
                    </li>
                </ul>
                <!-- Left links -->

                <div class="d-flex align-items-center">


                    <button type="button" class="btn btn-primary me-3">
            Sign up for free
            </button>

                    <a class="btn btn-primary px-3 me-2" href="https://github.com/mdbootstrap/mdb-ui-kit" role="button"><i class="fas fa-sign-in-alt"></i></a>


                    <a class="btn btn-dark px-3" href="https://github.com/mdbootstrap/mdb-ui-kit" role="button"><i class="fab fa-github"></i  ></a>


                </div>
            </div>
