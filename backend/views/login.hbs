        </div>
        <!-- Container wrapper -->
    </nav>
    <!-- Navbar -->

    <!-- end header section -->
    <main class="contenedor">
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-6 login-section-wrapper login-section">
                    <div class="login-wrapper my-auto">
                        <h1 class="login-title">Log in</h1>
                        <form method="post" action="/it/login">
                            <div class="form-group">
                                <label for="email">Email</label>
                                <input type="email" name="email" id="email" class="form-control" placeholder="email@example.com">
                            </div>
                            <div class="form-group mb-4">
                                <label for="password">Password</label>
                                <input type="password" name="password" id="password" class="form-control" placeholder="enter your passsword">
                            </div>
                            <input name="login" id="login" class="btn btn-block login-btn" type="submit" value="Login">
                        </form>
                        <a href="/tienda/restablecer-password" class="forgot-password-link">Olvide Contraseña?</a>
                        <p class="login-wrapper-footer-text">No tienes cuenta? <a href="/tienda/registro" class="text-reset">Registrame Aqui</a></p>
                    </div>
                </div>
                <div class="col-sm-6 px-0 d-none d-sm-block">
                    <img src="/img/TECLALOGINROSA (1).png" alt="login image" class="login-img" width="1920" width="1522">
                </div>
            </div>
        </div>
    </main>

